use App\Comparable;

class User implements Comparable
{
    private $name;
    private $id;

    public function __construct($id, $name)
    {
        $this->name = $name;
        $this->id = $id;
    }

    public function getName()
    {
        return $this->name;
    }

    public function getId()
    {
        return $this->id;
    }

    public function compareTo($user): bool
    {
        return $this->id === $user->id;
    }
}
